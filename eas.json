{
  "cli": {
    "version": ">= 7.1.3"
  },
  "build": {
    "production-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "production-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "staging-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "staging",
        "EXPO_PUBLIC_APP_NAME": "Unsiagram (Stg)",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_API_URL": "https://staging.unsiagram.mathiuskormasela.com/api/v1",
        "EXPO_PUBLIC_APP_URL": "https://staging.unsiagram.mathiuskormasela.com"
      }
    },
    "staging-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "staging",
        "EXPO_PUBLIC_APP_NAME": "Unsiagram (Stg)",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_API_URL": "https://staging.unsiagram.mathiuskormasela.com/api/v1",
        "EXPO_PUBLIC_APP_URL": "https://staging.unsiagram.mathiuskormasela.com"
      }
    },
    "development-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "development-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      }
    }
  },
  "submit": {
    "production": {}
  }
}